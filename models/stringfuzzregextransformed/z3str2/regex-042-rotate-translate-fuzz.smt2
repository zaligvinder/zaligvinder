(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.union (str.to_re "'\\'") (re.* (str.to_re "'")))))
(assert (str.in_re x (re.union (re.* (re.+ (str.to_re "x"))) (re.union (str.to_re "k%+\\u{'}") (str.to_re "m")))))
(assert (= 3 (str.len x)))
(check-sat)
