(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "UU") (str.to_re "nn")))))
(assert (= 6 (str.len x)))
(assert (not (= x "UUnnnn")))
(assert (not (= x "UUnnUU")))
(assert (not (= x "nnnnUU")))
(assert (not (= x "nnnnnn")))
(assert (not (= x "nnUUUU")))
(assert (not (= x "nnUUnn")))
(assert (not (= x "UUUUUU")))
(check-sat)
