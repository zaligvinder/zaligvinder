(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.* (re.++ (str.to_re "") (re.+ (str.to_re "L"))))))
(assert (str.in_re y (re.* (re.union (str.to_re "") (re.+ (str.to_re ""))))))
(assert (= (str.len x) (str.to_int y)))
(assert (not (= x y)))
(assert (= (str.to_int x) 0))
(check-sat)
