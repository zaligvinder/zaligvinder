(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "k") (str.to_re "`")))))
(assert (= 3 (str.len x)))
(assert (not (= x "k``")))
(assert (not (= x "k`k")))
(assert (not (= x "``k")))
(assert (not (= x "```")))
(assert (not (= x "`kk")))
(assert (not (= x "`k`")))
(assert (not (= x "kkk")))
(check-sat)
