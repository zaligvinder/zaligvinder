(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "+") (str.to_re "'\u{b}'")))))
(assert (= 3 (str.len x)))
(assert (not (= x "'\u{b}''\u{b}'+")))
(assert (not (= x "+'\u{b}'+")))
(assert (not (= x "+'\u{b}''\u{b}'")))
(assert (not (= x "'\u{b}''\u{b}''\u{b}'")))
(assert (not (= x "++'\u{b}'")))
(assert (not (= x "'\u{b}'+'\u{b}'")))
(assert (not (= x "+++")))
(check-sat)
