(set-logic ALL)
(declare-const x String)
(declare-const y String)
(assert (str.in_re x (re.+ (re.union (str.to_re "") (re.+ (str.to_re ""))))))
(assert (str.in_re y (re.+ (re.++ (str.to_re "b") (re.+ (str.to_re "x"))))))
(assert (= (str.len x) (str.len y)))
(assert (not (= x y)))
(assert (= (str.len x) 5))
(check-sat)
