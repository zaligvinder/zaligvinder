(set-logic ALL)
(declare-const x String)
(declare-const y String)
(declare-const m String)
(declare-const n String)
(assert (str.in_re x (re.* (re.union (str.to_re "::") (str.to_re ";;")))))
(assert (= 6 (str.len x)))
(assert (not (= x ";;;;::")))
(assert (not (= x "::;;::")))
(assert (not (= x "::;;;;")))
(assert (not (= x ";;;;;;")))
(assert (not (= x "::::;;")))
(assert (not (= x ";;::;;")))
(assert (not (= x "::::::")))
(check-sat)
